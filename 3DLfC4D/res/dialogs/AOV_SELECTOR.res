DIALOG AOV_SELECTOR{  NAME TDLG;    FIT_H; FIT_V; SCALE_V; SCALE_H;		GROUP	{		COLUMNS 1;		FIT_H; SCALE_H;		BORDERSIZE 50,10,50,0;						GROUP		{			COLUMNS 2;			FIT_H; SCALE_H;						GROUP			{			BORDERSIZE 10,0,0,0;			SCALE_H;			STATICTEXT { NAME OUTPUT;}			}						GROUP			{			SCALE_H; FIT_H; ALIGN_LEFT;			STATICTEXT { NAME DL_IMAGE_LAYER_TXT;}			}								}						GROUP		{			FIT_H; SCALE_H;			LISTVIEW DL_SELECTED_LAYERS_LISTVIEW {FIT_H; SCALE_H; SIZE -500,-120; };		}	}					GROUP	{		CENTER_H; CENTER_V;		COLUMNS 4; 		GROUP		{		BUTTON AOV_ADD{SIZE -100,-20; NAME ADDTEXT; }		}				GROUP		{		BUTTON AOV_REMOVE{SIZE -100,-20; NAME REMOVETEXT; }		}				GROUP		{		BUTTON AOV_DUPLICATE{SIZE -100,-20; NAME DUPLICATETEXT; }		}				GROUP		{		BUTTON AOV_VIEW{SIZE -100,-20; NAME VIEWTEXT; }		}	}}